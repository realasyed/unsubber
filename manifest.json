{
    "manifest_version": 3,
    "name": "Unsubber",
    "version": "1.0",
  
    "description": "Unsubs from all YouTube channels.",
    "homepage_url": "https://github.com/realasyed/unsubber",
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
  
    "permissions": ["activeTab"],
  
    "browser_action": {
      "default_icon": "icons/icon-48.png",
      "default_title": "Unsubber",
      "default_popup": "popup/"
    },
  
    "web_accessible_resources": [
    ]
  }
  